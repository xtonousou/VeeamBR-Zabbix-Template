    <templates>
        <template>
            <template>Template VEEAM - Backup and Replication - RESTful API</template>
            <name>Template VEEAM - Backup and Replication - RESTful API</name>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>VEEAM Jobs</name>
                </application>
                <application>
                    <name>VEEAM Repositories</name>
                </application>
            </applications>
            <discovery_rules>
                <discovery_rule>
                    <name>VEEAM job discovery</name>
                    <type>EXTERNAL</type>
                    <key>veeam_monitor.sh[&quot;jobs&quot;]</key>
                    <delay>1h</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>VEEAM Last Result Of &quot;{#JOB_NAME}&quot; Job</name>
                            <type>DEPENDENT</type>
                            <key>veeam_monitor.sh[&quot;jobs&quot;,&quot;{#JOB_NAME}&quot;, &quot;last_result&quot;]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>VEEAM Jobs</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.last_result</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>veeam_monitor.sh[&quot;jobs&quot;,&quot;{#JOB_NAME}&quot;]</key>
                            </master_item>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{str(Failed)}=1</expression>
                                    <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                                    <recovery_expression>{str(Failed)}=0</recovery_expression>
                                    <name>VEEAM job &quot;{#JOB_NAME}&quot; failed</name>
                                    <priority>HIGH</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{str(Success)}=1</expression>
                                    <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                                    <recovery_expression>{str(Success)}=0</recovery_expression>
                                    <name>VEEAM job &quot;{#JOB_NAME}&quot; finished successfully</name>
                                    <priority>INFO</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{str(Warning)}=1</expression>
                                    <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                                    <recovery_expression>{str(Warning)}=0</recovery_expression>
                                    <name>VEEAM job &quot;{#JOB_NAME}&quot; finished with warnings</name>
                                    <priority>AVERAGE</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>VEEAM Objects Count Of &quot;{#JOB_NAME}&quot; Job</name>
                            <type>DEPENDENT</type>
                            <key>veeam_monitor.sh[&quot;jobs&quot;,&quot;{#JOB_NAME}&quot;, &quot;object_count&quot;]</key>
                            <delay>0</delay>
                            <applications>
                                <application>
                                    <name>VEEAM Jobs</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.object_count</params>
                                </step>
                            </preprocessing>
