                                <key>veeam_monitor.sh[&quot;jobs&quot;,&quot;{#JOB_NAME}&quot;]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>VEEAM Status Of &quot;{#JOB_NAME}&quot; Job</name>
                            <type>DEPENDENT</type>
                            <key>veeam_monitor.sh[&quot;jobs&quot;,&quot;{#JOB_NAME}&quot;, &quot;status&quot;]</key>
