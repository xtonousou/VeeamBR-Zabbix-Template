                            <applications>
                                <application>
                                    <name>VEEAM Jobs</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.status</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>veeam_monitor.sh[&quot;jobs&quot;,&quot;{#JOB_NAME}&quot;]</key>
                            </master_item>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{str(disabled)}=1</expression>
                                    <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                                    <recovery_expression>{str(disabled)}=0</recovery_expression>
                                    <name>VEEAM job &quot;{#JOB_NAME}&quot; is disabled</name>
                                    <priority>INFO</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{str(inactive)}=1</expression>
                                    <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                                    <recovery_expression>{str(inactive)}=0</recovery_expression>
                                    <name>VEEAM job &quot;{#JOB_NAME}&quot; is inactive</name>
                                    <priority>INFO</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{str(running)}=1</expression>
                                    <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                                    <recovery_expression>{str(running)}=0</recovery_expression>
                                    <name>VEEAM job &quot;{#JOB_NAME}&quot; is running</name>
                                    <priority>INFO</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>VEEAM Type Of &quot;{#JOB_NAME}&quot; Job</name>
                            <type>DEPENDENT</type>
                            <key>veeam_monitor.sh[&quot;jobs&quot;,&quot;{#JOB_NAME}&quot;, &quot;type&quot;]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
