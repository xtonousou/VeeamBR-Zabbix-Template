                            <type>EXTERNAL</type>
                            <key>veeam_monitor.sh[&quot;repositories&quot;,&quot;{#REPO_NAME}&quot;]</key>
                            <delay>10m</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>VEEAM Repositories</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                    <trigger_prototypes>
                        <trigger_prototype>
                            <expression>{Template VEEAM - Backup and Replication - RESTful API:veeam_monitor.sh[&quot;repositories&quot;,&quot;{#REPO_NAME}&quot;, &quot;space_free&quot;].last()} * 100 / {Template VEEAM - Backup and Replication - RESTful API:veeam_monitor.sh[&quot;repositories&quot;,&quot;{#REPO_NAME}&quot;, &quot;space_total&quot;].last()} &lt; 5</expression>
                            <name>VEEAM repository &quot;{#REPO_NAME}&quot; has less than 5% of free space</name>
                            <priority>HIGH</priority>
                            <manual_close>YES</manual_close>
                        </trigger_prototype>
                        <trigger_prototype>
                            <expression>{Template VEEAM - Backup and Replication - RESTful API:veeam_monitor.sh[&quot;repositories&quot;,&quot;{#REPO_NAME}&quot;, &quot;space_free&quot;].last()} * 100 / {Template VEEAM - Backup and Replication - RESTful API:veeam_monitor.sh[&quot;repositories&quot;,&quot;{#REPO_NAME}&quot;, &quot;space_total&quot;].last()} &lt; 10</expression>
                            <name>VEEAM repository &quot;{#REPO_NAME}&quot; has less than 10% of free space</name>
                            <priority>AVERAGE</priority>
                            <manual_close>YES</manual_close>
