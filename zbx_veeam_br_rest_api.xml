                            <name>VEEAM repository &quot;{#REPO_NAME}&quot; has less than 15% of free space</name>
                            <priority>WARNING</priority>
                            <manual_close>YES</manual_close>
                        </trigger_prototype>
                    </trigger_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>Free storage space of repository {#REPO_NAME}</name>
                            <graph_items>
